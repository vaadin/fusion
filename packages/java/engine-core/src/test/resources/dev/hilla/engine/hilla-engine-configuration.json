{
  "baseDir": "base",
  "classPath": ["build/classes", "dependency"],
  "generator": {
    "plugins": {
      "disable": [
        {
          "path": "generator-jvm-plugin-disable"
        }
      ],
      "disableAllDefaults": true,
      "use": [
        {
          "path": "generator-jvm-plugin-use"
        }
      ]
    }
  },
  "parser": {
    "classPath": {
      "delimiter": ";",
      "override": false,
      "value": null
    },
    "endpointAnnotation": "dev.hilla.test.Endpoint",
    "endpointExposedAnnotation": "dev.hilla.test.EndpointExposed",
    "openAPIPath": "test-openapi.json",
    "plugins": {
      "disable": [
        {
          "configuration": null,
          "name": "parser-jvm-plugin-disable",
          "order": null
        }
      ],
      "disableAllDefaults": true,
      "use": [
        {
          "configuration": null,
          "name": "parser-jvm-plugin-use",
          "order": null
        }
      ]
    }
  },
  "buildDir": "build"
}
